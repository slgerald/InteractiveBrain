<Window x:Class="LEDPracticeAppWPFV1._0._1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LEDPracticeAppWPFV1._0._1"
        mc:Ignorable="d"
        Title="Poe Health Center " Height="592" Width="879">
    <Grid Margin="0,0,2,0" Height="562" VerticalAlignment="Top">
        <Button x:Name="onButton" Content="LED ON" HorizontalAlignment="Left" Margin="625,180,0,0" VerticalAlignment="Top" Width="75" Click="onButton_Click" Height="20" RenderTransformOrigin="0.533,-2.5"/>
        <Button x:Name="offButton" Content="LED OFF" HorizontalAlignment="Left" Margin="625,145,0,0" VerticalAlignment="Top" Width="75" Click="offButton_Click" Height="20"/>
        <Button x:Name="connectButton" Content="Connect" HorizontalAlignment="Left" Margin="720,39,0,0" VerticalAlignment="Top" Width="75" Click="connectButton_Click" Height="20"/>
        <Button x:Name="disconnectButton" Content="Disconnect" HorizontalAlignment="Left" Margin="720,80,0,0" VerticalAlignment="Top" Width="75" Click="disconnectButton_Click" Height="20"/>
        <ComboBox x:Name="comPortNumberComboBox" HorizontalAlignment="Left" Margin="486,39,0,0" VerticalAlignment="Top" Width="204" Height="22"/>
        <Label Content="COM PORT NUMBER" HorizontalAlignment="Center" Margin="345,35,399,0" VerticalAlignment="Top" Width="125" Height="26"/>
        <TextBox x:Name="messageTextBox" HorizontalAlignment="Left" Height="149" Margin="330,125,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="229"/>
        <StackPanel HorizontalAlignment="Left" Height="100" Margin="625,215,0,0" VerticalAlignment="Top" Width="100"/>
        <StackPanel x:Name ="StackPanel1" Height="757" Margin="-25,0,609,-195" VerticalAlignment="Top">
            <Button x:Name="middleSchoolButton" Content="5th - 8th Grade" Height="75" Click="middleSchoolButton"/>
            <Button x:Name="highSchoolButton" Content="9th - 12th Grade" Height="75" Click="highSchoolButton"/>
            <Button x:Name ="simulationButton" Content="Simulation" Height="75" Click="simulationButton"/>
            <Button x:Name="interactiveBrainButton" Content="Interactive Brain" Height="75" Click="interactiveBrainButton"/>
            <Button x:Name ="serialTestButton" Content="Serial Communications" Height="75" Click="serialTestButton"/>
        </StackPanel>

    </Grid>
</Window>
