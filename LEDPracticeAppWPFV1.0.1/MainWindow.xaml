<Window x:Class="LEDPracticeAppWPFV1._0._1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LEDPracticeAppWPFV1._0._1"
        mc:Ignorable="d"
        Title="Poe Health Center " Height="592" Width="879" Loaded="Window_Loaded">
    <Window.Resources>
        <CollectionViewSource x:Key="selectionInteractiveBrainViewSource" d:DesignSource="{d:DesignInstance {x:Type local:selectionInteractiveBrain}, CreateList=True}"/>
    </Window.Resources>
    <Grid Canvas.ZIndex=" 0">
        <Button x:Name="onButton" Content="LED ON" HorizontalAlignment="Left" Margin="642,341,0,0" VerticalAlignment="Top" Width="101" Click="onButton_Click" Height="48" RenderTransformOrigin="0.533,-2.5"/>
        <Button x:Name="offButton" Content="LED OFF" HorizontalAlignment="Left" Margin="642,260,0,0" VerticalAlignment="Top" Width="101" Click="offButton_Click" Height="46"/>
        <Button x:Name="connectButton" Content="Connect" HorizontalAlignment="Left" Margin="769,11,-1,0" VerticalAlignment="Top" Width="101" Click="connectButton_Click" Height="47"/>
        <Button x:Name="disconnectButton" Content="Disconnect" HorizontalAlignment="Left" Margin="769,107,-1,0" VerticalAlignment="Top" Width="101" Click="disconnectButton_Click" Height="48"/>
        <ComboBox x:Name="comPortNumberComboBox" HorizontalAlignment="Left" Margin="455,11,0,0" VerticalAlignment="Top" Width="274" Height="52"/>
        <Label Content="COM PORT NUMBER" HorizontalAlignment="Center" Margin="265,2,435,0" VerticalAlignment="Top" Width="169" Height="61"/>
        <TextBox x:Name="messageTextBox" HorizontalAlignment="Left" Height="350" Margin="245,212,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="308"/>
        <StackPanel x:Name="StackPanel1" HorizontalAlignment="Left" Height="400" VerticalAlignment="Top" Width="240">
            <Button x:Name="interactiveBrainButton" Content="Interactive Brain" Height="80" Click="interactiveBrainButton_Click"/>
            <Button x:Name="simulationButton" Content="Simulation" Height="80" Click="simulationButton_Click"/>
            <Button x:Name="serialCommsButton" Content="Serial Communications Test" Height="80" Click="serialCommsButton_Click"/>
            <Button x:Name="middleSchoolButton" Content="5th - 8th Grade" Height="80" Click="middleSchoolButton_Click"/>
            <Button x:Name="highSchoolButton" Content="9th - 12th Grade" Height="80" Click="highSchoolButton_Click"/>
        </StackPanel>
        <Canvas x:Name="myCanvas" Canvas.ZIndex="1" HorizontalAlignment="Left" Height="560" Margin="245,2,0,-1" VerticalAlignment="Top" Width="626" DataContext="{StaticResource selectionInteractiveBrainViewSource}"/>


    </Grid>
</Window>
