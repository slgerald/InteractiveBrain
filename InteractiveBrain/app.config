<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <sectionGroup name="userSettings" type="System.Configuration.UserSettingsGroup, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089">
      <section name="InteractiveBrain.Properties.Settings" type="System.Configuration.ClientSettingsSection, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" allowExeDefinition="MachineToLocalUser" requirePermission="false"/>
    </sectionGroup>
    <!-- For more information on Entity Framework configuration, visit http://go.microsoft.com/fwlink/?LinkID=237468 -->
    <section name="entityFramework" type="System.Data.Entity.Internal.ConfigFile.EntityFrameworkSection, EntityFramework, Version=6.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false"/>
  </configSections>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6"/>
  </startup>
  <userSettings>
    <InteractiveBrain.Properties.Settings>
      <setting name="defaultFunctionsFrontalLobe" serializeAs="String">
        <value>planning, reasoning, speech, voluntary movement (motor cortex is in the frontal lobe), problem solving, regulating emotions (the frontal lobe doesnâ€™t initiate the emotion, but it helps us control our emotions)"</value>
      </setting>
      <setting name="defaultHealthyBehaviorsFrontalLobe" serializeAs="String">
        <value>Reading, Problem-Solving games, choreography (like for ballet, Zumba), meditation
</value>
      </setting>
      <setting name="defaultUnhealthyBehaviorsFrontalLobe" serializeAs="String">
        <value/>
      </setting>
      <setting name="defaultFunctionsTemporalLobe" serializeAs="String">
        <value>Hearing, processing short term memory (hippocampus located in this lobe)</value>
      </setting>
      <setting name="defaultHealthyBehaviorsTemporalLobe" serializeAs="String">
        <value/>
      </setting>
      <setting name="defaultUnhealthyBehaviorsTemporalLobe" serializeAs="String">
        <value/>
      </setting>
      <setting name="defaultFunctionsParietalLobe" serializeAs="String">
        <value>sensory processing such as temperature, pressure, touch, &amp; pain</value>
      </setting>
      <setting name="defaultHealthyBehaviorsParietalLobe" serializeAs="String">
        <value/>
      </setting>
      <setting name="defaultUnhealthyBehaviorsParietalLobe" serializeAs="String">
        <value/>
      </setting>
      <setting name="defaultFunctionsOccipitalLobe" serializeAs="String">
        <value>visual Processing</value>
      </setting>
      <setting name="defaultHealthyBehaviorsOccipitalLobe" serializeAs="String">
        <value/>
      </setting>
      <setting name="defaultUnhealthyBehaviorsOccipitalLobe" serializeAs="String">
        <value/>
      </setting>
      <setting name="defaultFunctionsCerebellum" serializeAs="String">
        <value>Balance &amp; coordination</value>
      </setting>
      <setting name="defaultHealthyBehaviorsCerebellum" serializeAs="String">
        <value>Any type of physical exercise, activities that help improve attention span</value>
      </setting>
      <setting name="defaultUnhealthyBehaviorsCerebellum" serializeAs="String">
        <value/>
      </setting>
    </InteractiveBrain.Properties.Settings>
  </userSettings>
  <entityFramework>
    <defaultConnectionFactory type="System.Data.Entity.Infrastructure.LocalDbConnectionFactory, EntityFramework">
      <parameters>
        <parameter value="v13.0"/>
      </parameters>
    </defaultConnectionFactory>
    <providers>
      <provider invariantName="System.Data.SqlClient" type="System.Data.Entity.SqlServer.SqlProviderServices, EntityFramework.SqlServer"/>
      <provider invariantName="System.Data.SQLite.EF6" type="System.Data.SQLite.EF6.SQLiteProviderServices, System.Data.SQLite.EF6"/>
    </providers>
  </entityFramework>
  <system.data>
    <DbProviderFactories>
      <remove invariant="System.Data.SQLite.EF6"/>
      <add name="SQLite Data Provider (Entity Framework 6)" invariant="System.Data.SQLite.EF6" description=".NET Framework Data Provider for SQLite (Entity Framework 6)" type="System.Data.SQLite.EF6.SQLiteProviderFactory, System.Data.SQLite.EF6"/>
    <remove invariant="System.Data.SQLite"/><add name="SQLite Data Provider" invariant="System.Data.SQLite" description=".NET Framework Data Provider for SQLite" type="System.Data.SQLite.SQLiteFactory, System.Data.SQLite"/></DbProviderFactories>
  </system.data>
</configuration>
